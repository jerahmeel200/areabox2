{
  "name": "areabox-client",
  "description": "Next.js SSR React on Cloud Functions for Firebase with Firebase Hosting",
  "version": "1.0.0",
  "main": "dist/build/server/index.js",
  "license": "MIT",
  "scripts": {
    "signin": "yarn firebase login",
    "setup": "yarn firebase use --add",
    "build:client": "next build src/client --no-lint",
    "build:server": "babel src/server --out-dir dist/build/server --source-maps",
    "build:admin": "cd Admin/client && yarn build && cd ../..",
    "build:prod": "cross-env NODE_ENV=production yarn build",
    "copy:client": "cp -r dist/client dist/build/client/",
    "build": " yarn build:server && yarn build:client && yarn copy:client",
    "watch:client": "next src/client",
    "watch:server": "babel src/server --out-dir dist/server --source-maps --watch",
    "watch": "yarn watch:client",
    "serve": "cross-env NODE_ENV=production yarn firebase serve --only functions,hosting",
    "preview:hosting": "yarn predeploy && firebase hosting:channel:deploy areabox",
    "admin:hosting": "yarn predeploy && NODE_ENV=production yarn firebase deploy --only hosting:adminapp",
    "nextapp:hosting": "yarn predeploy && NODE_ENV=production yarn firebase deploy --only hosting:nextapp",
    "admin:function": "cd Admin/functions && firebase --debug deploy --only functions:admin && cd ../..",
    "predeploy": "rimraf dist/ && yarn build",
    "next": "next build --no-lint",
    "deploy": "cross-env NODE_ENV=production yarn build && firebase deploy --only hosting"
  },
  "engine": "16",
  "dependencies": {
